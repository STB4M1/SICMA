/* ===== Base ===== */
QWidget{
  font-family: &quot;Noto Sans JP&quot;;
  font-size: 10pt;
  color: #0F172A;            /* slate-900 */
  background: #F6F7FB;       /* soft app background */
}

/* ===== Card-like labels (image previews, de/ds boxes) ===== */
QLabel#labelImage,
QLabel#labelGrayscaleImage,
QLabel#labelBinaryImage,
QLabel#labelCroppedImage,
QLabel#labelDe,
QLabel#labelDs{
  background: #FFFFFF;
  border: 1px solid #E5E7EB; /* gray-200 */
  border-radius: 16px;
  padding: 8px;
}

/* Make preview frames look premium */
QLabel#labelImage,
QLabel#labelGrayscaleImage,
QLabel#labelBinaryImage,
QLabel#labelCroppedImage{
  border-radius: 18px;
}

/* Helper labels */
QLabel#labelImageSize,
QLabel#labelGrayScaledImageSize,
QLabel#labelBinarizedImageSize,
QLabel#labelCroppedImageSize,
QLabel#label_6{
  color: #64748B;            /* slate-500 */
}

/* ===== Tension readout ===== */
QLabel#labelTensionValue{
  background: #0B1220;       /* deep navy */
  color: #E5E7EB;            /* gray-200 */
  border: 1px solid #111827;
  border-radius: 16px;
  padding: 8px 10px;
  font-size: 20pt;
  font-weight: 700;
  letter-spacing: 0.4px;
}

/* =======================================================================
   Buttons: depth + slight glass highlight (keep your color palette)
   - 立体感：上が少し明るい / 下が少し暗い（同系色の範囲）
   - 透明感：上面の薄いハイライト（border + gradient）
   - 押下：沈み込み（border/padding変化）
   ======================================================================= */

/* ----- Common button base (applies to all buttons) ----- */
QPushButton{
  color: #FFFFFF;
  font-weight: 700;
  border-radius: 12px;

  /* base size */
  padding: 9px 12px;

  /* Default (your original was #111827). Use a subtle gradient in same family. */
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #1F2937, stop:0.45 #111827, stop:1 #0B1220);

  /* Glass highlight / bevel edges */
  border-top: 1px solid rgba(255,255,255,0.28);
  border-left: 1px solid rgba(255,255,255,0.18);
  border-right: 1px solid rgba(0,0,0,0.25);
  border-bottom: 2px solid rgba(0,0,0,0.45);
}

QPushButton:hover{
  /* slightly brighter top highlight */
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #263449, stop:0.45 #111827, stop:1 #070C16);
  border-top: 1px solid rgba(255,255,255,0.34);
}

QPushButton:pressed{
  /* sunken look */
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #0B1220, stop:0.55 #050814, stop:1 #000000);

  border-top: 2px solid rgba(0,0,0,0.55);
  border-left: 1px solid rgba(0,0,0,0.40);
  border-right: 1px solid rgba(0,0,0,0.40);
  border-bottom: 1px solid rgba(255,255,255,0.10);

  /* tiny press shift */
  padding-top: 10px;
  padding-bottom: 8px;
}

/* Focus ring (控えめ) */
QPushButton:focus{
  /* 既存の立体感を壊さず、うっすら輪郭だけ */
  border-top: 1px solid rgba(255,255,255,0.30);
  border-left: 1px solid rgba(255,255,255,0.20);
  border-right: 1px solid rgba(0,0,0,0.25);
  border-bottom: 2px solid rgba(0,0,0,0.45);
}

/* Disabled */
QPushButton:disabled{
  color: rgba(255,255,255,0.55);
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #475569, stop:1 #334155);
  border-top: 1px solid rgba(255,255,255,0.18);
  border-left: 1px solid rgba(255,255,255,0.10);
  border-right: 1px solid rgba(0,0,0,0.18);
  border-bottom: 2px solid rgba(0,0,0,0.22);
}

/* ----- Primary action buttons (Blue-600 family 유지) ----- */
/* 元の指定色：background #2563EB / hover #1D4ED8 を “立体” にする */
QPushButton#pushButtonSelectImage,
QPushButton#pushButtonCalcTensionStaufferFordham,
QPushButton#pushButtonCalcTensionMisak{
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #3B82F6, stop:0.45 #2563EB, stop:1 #1D4ED8);
  border-top: 1px solid rgba(255,255,255,0.35);
  border-left: 1px solid rgba(255,255,255,0.22);
  border-right: 1px solid rgba(0,0,0,0.22);
  border-bottom: 2px solid rgba(0,0,0,0.38);
}

QPushButton#pushButtonSelectImage:hover,
QPushButton#pushButtonCalcTensionStaufferFordham:hover,
QPushButton#pushButtonCalcTensionMisak:hover{
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #4F8DF7, stop:0.45 #1D4ED8, stop:1 #1E40AF);
  border-top: 1px solid rgba(255,255,255,0.42);
}

QPushButton#pushButtonSelectImage:pressed,
QPushButton#pushButtonCalcTensionStaufferFordham:pressed,
QPushButton#pushButtonCalcTensionMisak:pressed{
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #1D4ED8, stop:0.55 #1E40AF, stop:1 #1E3A8A);
}

/* ----- Accent: Trim (Emerald family 유지) ----- */
/* 元の指定色：background #10B981 / hover #059669 を “立体” にする */
QPushButton#pushButtonTrim{
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #34D399, stop:0.45 #10B981, stop:1 #059669);
  border-top: 1px solid rgba(255,255,255,0.36);
  border-left: 1px solid rgba(255,255,255,0.22);
  border-right: 1px solid rgba(0,0,0,0.22);
  border-bottom: 2px solid rgba(0,0,0,0.36);
}
QPushButton#pushButtonTrim:hover{
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #4AE0AE, stop:0.45 #059669, stop:1 #047857);
  border-top: 1px solid rgba(255,255,255,0.44);
}
QPushButton#pushButtonTrim:pressed{
  background: qlineargradient(x1:0, y1:0, x2:0, y2:1,
                              stop:0 #059669, stop:0.55 #047857, stop:1 #065F46);
}

/* ===== Line edits ===== */
QLineEdit{
  background:#FFFFFF;
  border:1px solid #D1D5DB;  /* gray-300 */
  border-radius:12px;
  padding:6px 10px;
  selection-background-color:#2563EB;
}
QLineEdit:focus{
  border:2px solid #2563EB;
  padding:5px 9px;           /* keep size stable */
}

/* ===== Slider ===== */
QSlider::groove:horizontal{
  height:8px;
  background:#E5E7EB;
  border-radius:4px;
}
QSlider::sub-page:horizontal{
  background:#93C5FD;        /* light blue */
  border-radius:4px;
}
QSlider::handle:horizontal{
  width:18px;
  margin:-6px 0;
  border-radius:9px;
  background:#2563EB;
  border:1px solid #1D4ED8;
}

/* ===== Log viewer ===== */
QPlainTextEdit{
  background:#0B1220;
  color:#E5E7EB;
  border:1px solid #111827;
  border-radius:14px;
  padding:8px;
  font-family:&quot;Noto Sans Mono&quot;;
  font-size:9.5pt;
}
